// 将设置放入此文件中以覆盖默认值和用户设置。
{
    // --- go begin --- 
    "go.buildOnSave": "off",
    "go.lintOnSave": "off",
    "go.lintTool": "golint",
    "go.vetOnSave": "off",
    "go.gocodeAutoBuild": true,
    "go.buildFlags": [],
    "go.lintFlags": [],
    "go.vetFlags": [],
    "go.testFlags": ["-v"],
    "go.formatTool": "gofmt",
    "go.goroot": "D:/go",
    "go.gopath": "${workspaceRoot}",
    "go.autocompleteUnimportedPackages": true,
    "go.gocodePackageLookupMode": "go",
    "go.gotoSymbol.includeImports": true,
    "go.useCodeSnippetsOnFunctionSuggest": true,
    "go.inferGopath": false,
    "go.useCodeSnippetsOnFunctionSuggestWithoutType": true,
    "go.liveErrors": {
        "enabled": true,
        "delay": 0
    },
    "go.useLanguageServer": true,
    "files.exclude": {
        "src/github.com/*": false,
        "src/golang.org/*": true,
        "src/sourcegraph.com/*": true,
        "src/google.golang.org/*": true,
        "pkg/**/*": true,
        "src/github.com/micro/go-plugins/registry/etcdv3": false,
        "src/go.etcd.io/*": true,
    }
}